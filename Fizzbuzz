#include<iomanip>
#include<string>
#include<iostream>

using namespace std;

int main()
{

	int x = 0; // initializing x 

	 cout<<"what is the range of numbers "<<endl;
     cin>>x; // sets range 

	for(int i = 0; i <= x ; i++) 
	{
       if( i % 3 && i % 5 == 0 ) // no remander when divided by three or five
	   {
		  cout<<"FizzBuzz"<<endl;
	   }
	   else if ( i % 5 == 0)     // no remander when divided by five
	   {
	      cout<<"Buzz"<<endl;
	   }
	   else if( i % 3 == 0 )   // no remander when divided by three
	   {
		cout<<"Fizz"<<endl;
	   }
	   else
	   {
	     cout<<i<<endl;      // other wise output position i 
	   }

	}


system("pause"); //pauses screen 
return(0);       // returns null 
}
